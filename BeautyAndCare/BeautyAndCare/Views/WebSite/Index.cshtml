@using BeautyAndCare.Models;
@using System.Linq;

@model  tblAll
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/LayOutNew.cshtml";
    WebSiteEntities db = new WebSiteEntities();
    var queryMenu = from data in db.tblMenus
                    where data.StatusMenu == 1
                    orderby data.OrderMenu ascending
                    select data;

}

<!--HEADER--->
@Html.Partial("Header", queryMenu.ToList())
<!--END HEADER--->
<!--CONTAINER--->
<div class="container">
    <div class="row">
        <div id="content" class="col-sm-12">


            @Html.Partial("Slider")
         
            <!--NewProducts--->
            @Html.Partial("NewProducts", Model)
            <!--NewProducts--->
            @Html.Partial("Blogs", Model)

        </div>
    </div>
</div>
<!--END CONTAINER--->
<!-- FOOTER--->
@Html.Partial("Footer")
<!--END FOOTER--->
